package DataStructures;

public class TwoDArray {

	public static void main(String[] args) {

		final int COUNTRIES=7;
		final int MEDALS=3;
		
		//int [][] count = new int[COUNTRIES][MEDALS];
		String [] countries ={"Cannada", "China", "Japan", "Russia", "Switzerland", "Ukraine", "United States"};
		
		 int [][] count = {{0,0,1},
						{0,1,1},
						{1,0,0},
						{3,0,1},
						{0,1,0},
						{0,0,1},
						{0,2,0}
						};
		 
		 System.out.println("Gold    Silver   Bronze");
		
		for(int i =0; i<MEDALS; i++){
			for(int j=0; j<COUNTRIES;j++){
				System.out.printf("%8d %8d %8d",count,count,count);
			}
			
			System.out.println();
		}
		

	}

}
